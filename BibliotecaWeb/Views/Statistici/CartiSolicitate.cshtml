@model BibliotecaWeb.Models.Statistici.CartiSolicitateModel

@{
    ViewData["Title"] = "CartiSolicitate";
}

<h2>Carti Solicitate</h2>

<div class="row">
    <div class="col-md-4">
        @{
        for (int i = 0; i < Model.carti.Count(); i++)
        {
            <div>@Model.carti[i].Titlu</div>
            <br />
    }
        }
    </div>
</div>

<div>
    <a asp-action="Index">Inapoi la Statistici</a>
</div>


