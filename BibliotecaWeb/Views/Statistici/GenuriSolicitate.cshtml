@model BibliotecaWeb.Models.Statistici.GenuriSolicitateModel

@{
    ViewData["Title"] = "GenuriSolicitate";
}

<h2>Genuri Solicitate</h2>

<div class="row">
    <div class="col-md-4">
        @{
            for (int i = 0; i < Model.genuri.Count(); i++)
            {
                <div>@Model.genuri[i].Descriere</div>
                <br />
            }
        }
    </div>
</div>

<div>
    <a asp-action="Index">Inapoi la Statistici</a>
</div>

