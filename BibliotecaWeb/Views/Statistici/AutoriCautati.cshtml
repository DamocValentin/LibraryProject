@model BibliotecaWeb.Models.Statistici.AutoriCautatiModel
@{
    ViewData["Title"] = "AutoriCautati";
}

<h2>Autori Cautati</h2>

<div class="row">
    <div class="col-md-4">
        @{
            for (int i = 0; i < Model.autori.Count(); i++)
            {
                <div>@Model.autori[i].Nume @Model.autori[i].Prenume </div>
                <br />
            }
        }
    </div>
</div>

<div>
    <a asp-action="Index">Inapoi la Statistici</a>
</div>